# Media control

XF86AudioPlay
  playerctl play-pause

XF86AudioNext
  playerctl next

XF86AudioPrev
  playerctl previous

XF86AudioMute
  pulsemixer --toggle-mute

XF86Calculator
  alacritty -e R

shift + F2
  ~/.scripts/system/volumedown

shift + F3
  ~/.scripts/system/volumeup

# Screenshots

Print
  flameshot full -c

alt + Print
  flameshot gui

shift + Print
  flameshot full -c -p ~/Pictures/screenshots/

# Other

alt + space
  ~/.scripts/system/dclip
